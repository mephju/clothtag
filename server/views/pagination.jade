ul
	- var numPages = Math.ceil(numRows / 12)
	- var nextPage = parseInt(page) + 1
	- var prevPage = page - 1
	- if(page == 1)
		li
			a.disabled(href='/') &laquo;
		li 
			a(href='/?page=' + nextPage) &raquo; 
	- else if(page > 1 && page < numPages)
		li
			a(href='/' + prevPage) &laquo;
		li 
			a(href='/?page=' + nextPage) &raquo;
	- else
		li
			a(href='/?page=' + nextPage) &laquo;
		li 
			a.disabled(href='/') &raquo;